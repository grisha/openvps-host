all:
	@cd ohchkpwd && $(MAKE)
	@cd pace && $(MAKE)
	@cd wrapper && $(MAKE)

install:
	@cd ohchkpwd && $(MAKE) install
	@cd pace && $(MAKE) install
	@cd wrapper && $(MAKE) install

clean:
	rm -rf $(OBJS) core *~ .libs *.o *.slo *.lo *.la
	@cd ohchkpwd && $(MAKE) clean
	@cd pace && $(MAKE) clean
	@cd wrapper && $(MAKE) clean

distclean: clean
	rm -f Makefile
	@cd ohchkpwd && $(MAKE) distclean
	@cd pace && $(MAKE) distclean
	@cd wrapper && $(MAKE) distclean

